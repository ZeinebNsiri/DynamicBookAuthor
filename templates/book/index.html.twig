
<table>
        <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Publication Date</th>
        <th>Enabled</th>
        <th>Action</th>
        <th>Author</th>
        </tr>
    
    {% for Book in Books %}

    <tr>
    <td>{{Book.id}}</td>
    <td>{{Book.title}}</td>
    <td>{{Book.publicationDate | date('Y-m-d') }}</td>
    <td>{{Book.enabled}}</td>
    <td><a href='{{ path ('app_deleteBook' , { 'id':Book.id } ) }}'>delete</a><td>
    <td><a href='{{ path ('app_updateBook' , { 'id':Book.id } ) }}'>update</a><td>
    <td>{{Book.author.username}}</td>
    </tr>

    {% endfor %}
    </table>
